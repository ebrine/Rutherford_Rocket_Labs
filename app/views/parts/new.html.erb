<section id="new-parts-page">
  <%= form_for :order, url: parts_path, html: {class: 'new-parts-form'} do |f| %>
  <h2>Parts Order Form</h2>
  <%= render '/layouts/flash' %>

  <ul class="new-parts-ul">
    <%= render 'new_line' %>
  </ul>

  <%= f.submit "Add New Line", class: 'submit-button new-part' %>
  <%= f.submit "Place Order", class: 'submit-button' %>
  <% end %>
</section><!-- end new order page -->
