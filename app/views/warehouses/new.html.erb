<section id="warehouse-new-page">
  <aside class="new-warehouse-container">
    <div class="warehouse-details">
    </div><!-- end warehouse details -->
    <%= form_for :warehouse, url: warehouses_path do |f| %>
    <h1>New Warehouse</h1>
    <% if defined? @errors %>
    <div class="errors">
      <% @errors.each do |error| %>
      <p><%= error %></p>
    </div>
    <% end %>
    <% end %>
      <%= f.label :city%>
      <%= f.text_field :city %>
      <%= f.submit class: 'submit-button' %>
    <% end %>
  </aside><!-- end new warehouse container -->
</section>
